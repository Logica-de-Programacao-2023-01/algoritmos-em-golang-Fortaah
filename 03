package main

import "fmt"

func main() {
	var peso float64
	var altura float64
	var x float64
	fmt.Print("Para sabermos o seu IMC precisamos de seu peso e sua altura, informe nessa ordem os dados.")
	fmt.Scan(&peso)
	fmt.Scan(&altura)
	x = altura * altura
	fmt.Println("O seu indice de IMC deu:", peso/x)
}
